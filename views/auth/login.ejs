<% include ../partials/header-top %>

<title>yo! favo | Login</title>

<% include ../partials/header-bottom %>

<div class="row"></div>

<div class="row">
  <div class="col s6 offset-s3 my-form-wrapper">
    <form action="/auth/login" method="POST">
      <div class="input-field">
        <i class="material-icons prefix">mail_outline</i>
        <input class="validate" id="authEmail" type="email" name="email" value="q@q.com">
        <label for="authEmail">Email</label>
      </div>

      <div class="input-field">
        <i class="material-icons prefix">lock_outline</i>
        <input class="validate" id="authPassword" type="password" name="password" value="password">
        <label for="authPassword">Password</label>
      </div>

      <div class="my-form-submit-wrapper center-align">
        <input class="center-align my-form-submit" type="submit">
      </div>
    </form>
  </div>
</div>

